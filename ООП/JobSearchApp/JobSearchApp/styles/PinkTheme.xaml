<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colors for button -->
    <SolidColorBrush x:Key="BackgroundColorBtn" Color="#de71f4"/>
    <SolidColorBrush x:Key="BorderColor" Color="#de71f4"/>
    <SolidColorBrush x:Key="MouseEneterColor" Color="#e9b1fb"/>
    <SolidColorBrush x:Key="MouseLeaveColor" Color="#de71f4"/>
    <SolidColorBrush x:Key="FontColor" Color="White"/>
    
    <!-- Colors for textbox -->
    <SolidColorBrush x:Key="IsFocusedColor" Color="#f5dcfb"/>
    <SolidColorBrush x:Key="BackgroundColorTextbox" Color="White"/>

    <!-- Background Color -->
    <SolidColorBrush x:Key="BackgroundColorWindow" Color="#f6e5fd"/>

    <!-- Animation Color-->
    <Color x:Key="HoverColor">#e9b1fb</Color>
    <Color x:Key="OutHoverColor">#de71f4</Color>
</ResourceDictionary>