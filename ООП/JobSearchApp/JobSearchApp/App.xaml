<Application x:Class="JobSearchApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:JobSearchApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary/Eng_Dictionary.xaml"/>
                <ResourceDictionary Source="Dictionary/Ru_Dictionary.xaml"/>
                <ResourceDictionary Source="Styles/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Styles/TextboxStyle.xaml"/>
                <ResourceDictionary Source="Styles/PinkTheme.xaml"/>
                <ResourceDictionary Source="Styles/BlueTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
